<%= render "shared/static_header" %>

<section class="devise-section">
  <div class="signin">
    <div class="row">
    	<div class="col-md-4 col-md-offset-4 col-sm-12">
    		<div class="sign-form">
    			<form>
            <input type="test" name="" placeholder="&#xf0e0; Email Address" class="sign-app-input sign_in_email">
            <input type="test" name="" placeholder="&#xf023; Password" class="sign-app-input sign_in_password">
            <input type="test" name="" placeholder="&#xf023; Password confirmation" class="sign-app-input sign_in_password">
            <p class="sign-up-bottom-link">By signing up, you agree to Moober's <span>Terms & Condition </span> and <span>Privacy Policy</span></p>
            <input type="submit" name="SIGN UP" class="web-login-btn">
            <p class="option-link">OR</p>
            <button class="web-facebook-btn" id="fb_sign_in"><span><i class="fa fa-facebook-official"></i></span> <a href="javascript:;">Sign in with Facebook</a></button>
            <p class="signin-link">Already Do you have account? Sign In</p>
          </form>
    		</div>
    	</div>
  </div>
</section>

<section class="devise-footer">
  <%= render 'shared/devise_footer' %>
</section>