<div class="search-map">
    <div class="row pd-15">
      <div class="col-md-4 search-map-div new-form">
        <p><span><img src="/assets/map-icon2.png"></span>Moving from</p>
        <input type="text" name="" id="start" placeholder="Pickup address" onFocus="geolocate()" >
      </div>
      <div class="col-md-4 search-map-div new-form">
        <p><span><img src="/assets/map-icon1.png"></span>Moving to</p>
        <input type="" name="" id="end" placeholder="Destination address" onFocus="geolocate()">
      </div>
      <div class="col-md-4 search-map-div" align="center">
        <button class="hidden-btn" id="map_search_btn"></button>
        <a href='#room-type-section' id="size_of_move" data-section_id="#map" class="hidden-btn smooth_scroll">CONTINUE</a>
        <a class="image-div-submit map-section-btn" id="start-end">CONTINUE</a>
      </div>
    </div>
  </div>
<!-- </section> -->
<section class="slide" id="room-type-section">
  <div class="row">
    <div class="mt-40">
      <div class="container">
        <div class="row">
          <div class="col-md-12 mt-20" align="center">
            <div class="start-heading">
              <h4><span class="span1"><img class="icon-image" src="/assets/icon1.jpg"></span><span class="span2"> What's the size of your move? </span><span class="span3"><img class="icon-image back_btn" src="/assets/back btn.jpg" data-back_section="#map" data-current_section="#room-type-section"></span></h4>
            </div>
          </div>
          <div class="scroll_div">
            <div class="row room_image">
              <div class="col-md-4">
                <div class="col-md-12 mt-25">
                  <img class="move-type" src="/assets/add-image.jpg">
                  <div class="check-div">
                    <div class="ins-image-align">
                      <input id="single-item" type="checkbox" class="new-app-ins item1" name="movesize" value="single item" data-value="1">
                      <label for="single-item">A single item</label>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-md-4">
                <div class="col-md-12 move-type mt-25">
                  <div class="check-div">
                    <div class="ins-image-align">
                      <input id="studio" type="checkbox" class="new-app-ins item2" name="movesize" value="studio" data-value="2">
                      <label for="studio">Studio</label>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-md-4">
                <div class="col-md-12 move-type mt-25">
                  <div class="check-div">
                    <div class="ins-image-align">
                      <input id="normal" type="checkbox" class="new-app-ins item3" name="movesize" value="normal" data-value="3">
                      <label for="normal"></label>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="bottom_div">
            <div class="row">
              <div class="col-md-4"></div>
              <div class="col-md-4">
                <div align="center">
                  <a href='#photos-section' id="room_type" data-section_id="#room-type-section" class="hidden-btn smooth_scroll">CONTINUE</a>
                  <a class="new-next-btn room_type_check" id="move-size">CONTINUE</a>
                </div>
              </div>
              <div class="col-md-4"></div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<section class="slide" id="photos-section">
  <div class="mt-40">
    <div class="container">
      <div class="row">
        <div class="col-md-12" align="center">
          <div class="start-heading1">
            <h4><span class="span1"><img class="icon-image" src="/assets/icon2.jpg"></span><span class="span2">Take photos of your space.</span><span class="span3"><img class="icon-image back_btn" src="/assets/back btn.jpg" data-back_section="#room-type-section" data-current_section="#photos-section"></span><span class="size-of-move-val-check-link"><a href='#moving-extra-section' data-section_id="#photos-section" class="smooth_scroll" id="skip-upload-images">Skip</a></span></h4>
            <!-- <input type="file" class="hidden-btn" name="" id="image_upload" multiple> -->
            <%= form_for MovePhoto.new, url: take_photos_path, html: { multipart: true, class: "file-form" }, remote: true do |f| %>
              <%= f.file_field :avatar, multiple: true, id: "image_upload" %>
              <%= f.submit "Submit", class: "file-upload-btn" %>
            <% end %>
            <!-- <p>(20 max)</p> -->
          </div>
        </div>
        <div class="scroll_div_new">
          <div class="row">
            <div class="image-content">
              
            </div>
            <div class="col-md-4 image_upload_btn">
              <div class="col-md-12 portfolio-item">
                <img class="" src="/assets/add-image.jpg" alt="">
              </div>
            </div>
          </div>
        </div>
        <div class="bottom_div">
          <div class="row">
            <div class="col-md-4"></div>
            <div class="col-md-4">
              <div align="center" class="size-of-move-val-check-div">
                <a href='#moving-extra-section' id="add_images" data-section_id="#photos-section" class="hidden-btn smooth_scroll">CONTINUE</a>
                <a class="image-div-submit add_images_section" id="move-images">CONTINUE</a>
              </div>
            </div>
            <div class="col-md-4"></div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<section class="slide" id="moving-extra-section">
  <div class="mt-40">
    <!-- <p>moving-extra</p> -->
    <div class="container">
      <div class="row">
        <div class="col-md-12" align="center">
          <div class="start-heading">
            <h4><span class="span1"><img class="icon-image" src="/assets/icon3.jpg"></span><span class="span2">Will there be any extra large items?</span><span class="span3"><img class="icon-image back_btn" src="/assets/back btn.jpg" data-back_section="#photos-section" data-current_section="#moving-extra-section"></span></h4>
            <!-- <p>(e.g Piano)</p> -->
          </div>
        </div>
      <div class="scroll_div">
        <div class="row">
          <div class="col-md-4"></div>
          <div class="col-md-4">
            <div class="ins-align">
              <input id="yes" type="checkbox" class="new-app-ins moving-extra-model-yes">
              <label for="yes">Yes</label><br />
            </div>
            <div class="new_popup_success_content">
              <div class="ins-align">
                <input id="no" type="checkbox" class="new-app-ins moving-extra-model-no">
