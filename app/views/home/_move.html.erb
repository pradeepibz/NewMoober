<script type="text/javascript">
  $(".more_info").click(function () {
    var $title = $(this).find(".title");
    if (!$title.length) {
        $(this).append('<span class="title">'+ '<i class="fa fa-times" aria-hidden="true"></i>' + $(this).attr("title") +'</span>');
    } else {
        $title.remove();
    }
  });
  $(document).bind('click', function(e) {
    if($(e.target).closest('p').length == 0) {
      $(".title").remove();
    }
  });

  $("#date3").scroller({ theme: 'ios', lang: 'EN', preset: 'datetime' });

</script>

