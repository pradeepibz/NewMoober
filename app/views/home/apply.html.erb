<%= render "shared/static_header" %>

<section class="slide" style="background:#f2f2f2;">
  <div class="contact">
    <div class="row">
      <div class="col-md-12">
        <div class="contact-cover" style="width:100%;background-image:url(/assets/moober_background_image.jpg);"></div>
        <div class="contact-title">APPLY TO EARN ON MOOBER</div>
      </div>
      <div class="col-md-12">
        <div class="contact-description">
          Ready to increase opportunities and volume for your moving business? Tell us a bit about your company and we'll be in touch within 24 hours.
        </div>
      </div>
  </div>
</section>

<section class="slide">
  <div class="mt-10">
    <div class="row">
      <div class="col-md-6 col-md-offset-3">
        <div class="contact-form text-center">
          <h1>Get Started</h1>
          <form class="form-group">
            <input type="text" name="" placeholder="Full Name">
            <input type="text" name="" placeholder="Company Name">
            <input type="text" name="" placeholder="Site Url">
            <input type="email" name="" placeholder="Email">
            <input type="text" name="" placeholder="City">
            <select>
              <option>Select</option>
            </select>
            <input type="text" name="" placeholder="Phone Number">
            <input type="submit" name="Submit" >
          </form>
        </div>
      </div>
    </div>
  </div>
</section>

<section class="slide">
  <div class="mt-10">
    <div class="row">
      <%= render "shared/company_logos" %>
    </div>
  </div>
</section>

<section class="slide">
  <%= render 'shared/footer' %>
</section>

<%= render 'shared/scrollify' %>